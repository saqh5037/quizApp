{
  "name": "aristotest",
  "version": "1.1.0",
  "description": "AristoTest - Interactive Learning Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "deploy:qa": "./scripts/qa-deployment/03-deploy-robust.sh"
  },
  "keywords": ["education", "quiz", "real-time", "assessment"],
  "author": "AristoTest Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}